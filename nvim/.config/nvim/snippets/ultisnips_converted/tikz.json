{
  "tp": {
    "prefix": "tp",
    "description": "TikZ picture environment",
    "body": [
      "\\begin{tikzpicture\\}",
      "\t$1",
      "\\end{tikzpicture\\}",
      "$0"
    ],
    "luasnip": {
      "autotrigger": true
    }
  },
  "dx": {
    "prefix": "dx",
    "description": "\\pgfmathsetmacro for tikz pictures",
    "body": "\\pgfmathsetmacro{\\dx\\}{${1:1}\\}$0",
    "luasnip": {
      "autotrigger": true
    }
  }
}
